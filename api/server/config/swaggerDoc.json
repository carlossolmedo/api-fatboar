{
    "openapi": "3.0.0",
    "info": {
        "title": "Fatboar API",
        "description": "API to restaurant's game",
        "version": "1.0.1"
    },
    "servers": {
        "url": "http://localhost:3002/api/v1",
        "description": "Development server"
    },
    "paths": {
        "/users": {
            "get": {
                "summary": "Returns a list of all users",
                "responses": {
                    "200": {
                        "description":"API to restaurant's game",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
